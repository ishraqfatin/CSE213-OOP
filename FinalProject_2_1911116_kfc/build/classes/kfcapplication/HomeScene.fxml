<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="kfcapplication.HomeSceneController">
    <children>
        <Label fx:id="label" layoutX="126" layoutY="120" minHeight="16" minWidth="69" />
      <Label alignment="CENTER" layoutX="161.0" layoutY="103.0" prefHeight="17.0" prefWidth="69.0" text="LOGIN">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="idTextField" alignment="CENTER" layoutX="126.0" layoutY="170.0" promptText="ID" />
      <TextField fx:id="passwordTextField" alignment="CENTER" layoutX="127.0" layoutY="214.0" promptText="Paswword" />
      <ComboBox fx:id="userComboBox" layoutX="127.0" layoutY="263.0" prefWidth="150.0" promptText="USER" />
      <ComboBox fx:id="branchComboBox" editable="true" layoutX="127.0" layoutY="303.0" onMouseClicked="#loginBranchClick" prefWidth="150.0" promptText="BRANCH" />
      <Label fx:id="errorLabel" alignment="CENTER" layoutX="127.0" layoutY="457.0" prefHeight="17.0" prefWidth="150.0" />
      <Button alignment="CENTER" layoutX="168.0" layoutY="391.0" mnemonicParsing="false" onAction="#loginButtonClick" prefHeight="25.0" prefWidth="69.0" text="Login" />
      <Button alignment="CENTER" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#adminButtonClick" opacity="0.0" prefHeight="17.0" prefWidth="59.0" text="Admin" />
    </children>
</AnchorPane>
